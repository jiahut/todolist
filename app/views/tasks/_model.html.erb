<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
	<h3><%= title %></h3>
</div>
<div class="modal-body">
	<%= render 'from' %>
</div>
<div class="modal-footer">
	<button id="task-cancel-btn"type="button" data-dismiss="modal" class="btn">Cancel</button>
	<button id="task-save-btn" type="button" class="btn btn-primary">Save</button>
</div>